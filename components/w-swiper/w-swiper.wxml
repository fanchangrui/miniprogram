<!--components/w-swiper/w-swiper.wxml-->
<swiper class="swiper" 
        circular 
        autoplay
        interval='3000' 
        duration="300" 
        indicator-dots="true"
        indicator-active-color="#ff5777"
        >
    <block wx:for="{{list}}" wx:key="item">
        <swiper-item class="swiper-item">
            <image src="{{item.image}}" mode="widthFix"></image>
        </swiper-item>
    </block>
</swiper>
